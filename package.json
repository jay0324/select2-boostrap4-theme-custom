{
  "name": "select2-boostrap4-theme-custom",
  "version": "1.0.0",
  "description": "select2-bootstrap4-theme 自訂修改方式",
  "main": "",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode development",
    "build:prod": "webpack --config webpack.config.js --mode production",
    "build:both": "npm run build && npm run build:prod",
    "prepare": "ln -sf ../dist/select2-custom.css docs/",
    "watch": "npm run prepare && npm run build -- --watch",
    "lint": "stylelint ./src/**/*.{scss,sass,css}",
    "lint:fix": "stylelint ./src/**/*.{scss,sass,css} --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jay0324/select2-boostrap4-theme-custom.git"
  },
  "keywords": [
    "select2",
    "theme",
    "bootstrap",
    "bootstrap4",
    "css",
    "scss",
    "custom"
  ],
  "author": "Jay Hsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jay0324/select2-boostrap4-theme-custom/issues"
  },
  "homepage": "https://github.com/jay0324/select2-boostrap4-theme-custom#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.3",
    "bootstrap": "^4.6.0",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.4",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-twbs-bootstrap": "^2.1.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0"
  },
  "jsdelivr": "dist/select2-awselect.min.css"
}
